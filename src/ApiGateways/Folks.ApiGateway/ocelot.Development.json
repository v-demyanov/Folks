{
  "Routes": [
    {
      "DownstreamPathTemplate": "/{path}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "folks.chatservice.api",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/chatservice/{path}",
      "UpstreamHttpMethod": [
        "GET",
        "POST",
        "PUT"
      ]
    },
    {
      "DownstreamPathTemplate": "/{path}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "folks.identityservice.api",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/identityservice/{path}",
      "UpstreamHttpMethod": [
        "GET",
        "POST",
        "PUT"
      ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:8000"
  }
}
